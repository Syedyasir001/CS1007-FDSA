class Node: 
    def __init__(self, data): 
        self.data = data 
        self.next = None  

class LinkedList: 
    def __init__(self): 
        self.head = None  

    def append_node(self, data): 
        """Appends a node at the end of the list."""
        new_node = Node(data) 
        if not self.head:  
            self.head = new_node
            return
        last = self.head
        while last.next: 
            last = last.next
        last.next = new_node  

    def search_node(self, data):
        """Search for a node with the given value."""
        current = self.head
        while current:
            if current.data == data:  
                return True
            current = current.next
        return False  

    def display_list(self): 
        """Displays all nodes in the linked list."""
        current = self.head
        if not current:
            print("The list is empty.")
            return
        while current:
            print(current.data, end=" -> " if current.next else "")
            current = current.next
        print()  
# Testing the linked list
linked_list = LinkedList()

# Append nodes to the list
linked_list.append_node(50)
linked_list.append_node(60)
linked_list.append_node(70)

# Display the list
print("Linked list after appending 50, 60, 70:")
linked_list.display_list()

# Search for a node
print("Searching for node with value 60:", linked_list.search_node(60))   
print("Searching for node with value 90:", linked_list.search_node(90)) 
